    <modelVersion>4.0.0</modelVersion>

    <artifactId>my-app-backend</artifactId>

    <properties>
        <java.version>1.8</java.version>
    </properties>

    <repositories>
    <repository>
        <id>com.teamdev</id>
        <url>https://europe-maven.pkg.dev/jxbrowser/releases</url>
    </repository>
    </repositories>



    <dependencies>
        <dependency>
